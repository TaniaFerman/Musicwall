<h1>list here</h1>
<% @lists.each do |list| %>
<div class="list">
  <h3><%= list.song %> <small>(by: <%= list.author %>)</small></h3>
  <p>
    <%= list.url %>
  </p>
  <p>
    <%= list.created_at %>
  </p>
  <p>
    <%= list.updated_at %>
  </p>
</div>

<% end %>
<a href="/list/new">Add a new song!</a>